<?xml version="1.0" ?>
<robot name="carA" xmlns:xacro="http://www.ros.org/wiki/xacro">
  

<xacro:include filename="$(find car_description)/carA/car.gazebo" />
<xacro:include filename="$(find car_control)/launch/car_control.xacro" />
<xacro:include filename="$(find car_description)/carA/material.xacro" />
  
  <joint name="carA__link_0_JOINT_0" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__link_1"/>
    <origin rpy="0       0       0" xyz="0.425 0.225  0.08"/>
    <axis xyz="0  0  0"/>
  </joint>

  <joint name="carA__link_0_JOINT_1" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__link_1_clone"/>
    <origin rpy="0       0       0" xyz="0.425   -0.225  0.08"/>
    <axis xyz="0  0  0"/>
  </joint>

  <joint name="carA__link_0_JOINT_2" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__link_1_clone_0"/>
    <origin rpy="0       0       0" xyz="-0.425  -0.225  0.08"/>
    <axis xyz="0  0  0"/>
  </joint>

  <joint name="carA__link_0_JOINT_3" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__link_1_clone_1"/>
    <origin rpy="0       0       0" xyz="-0.425  0.225  0.08"/>
    <axis xyz="0  0  0"/>
  </joint>

  <joint name="carA__link_0_JOINT_4" type="continuous">
    <parent link="carA__base_link"/>
    <child link="carA__right_wheel"/>
    <origin rpy="1.5707   0       0" xyz="0 -0.35  0.15"/>
    <axis xyz="0  0  -1"/>
  </joint>

  <joint name="carA__link_0_JOINT_5" type="continuous">
    <parent link="carA__base_link"/>
    <child link="carA__left_wheel"/>
    <origin rpy="1.5707   0       0" xyz="0  0.35  0.15"/>
    <axis xyz="0  0  -1"/>
  </joint>

  <joint name="carA__link_0_JOINT_7" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__front_hokuyo"/>
    <origin rpy="0       0       0.5404195002" xyz="0.45  0.25  0.48"/>
    <axis xyz="0  0  0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>

  <joint name="carA__link_0_JOINT_8" type="fixed">
    <parent link="carA__base_link"/>
    <child link="carA__rear_hokuyo"/>
    <origin rpy="0       0       -2.6009805" xyz="-0.45  -0.25  0.48"/>
    <axis xyz="0  0  0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>

  <link name="carA__base_link">
    <inertial>
      <mass value="50"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="1.875" ixy="0" ixz="0" iyy="4.541667" iyz="0" izz="5.6667"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0       0       0" xyz="0  0 0.3"/>
      <geometry>
        <box size="1 0.6 0.3"/>
      </geometry>
    </collision>
    <visual >
      <origin rpy="0       0       0" xyz="0  0  0.3"/>
      <geometry>
        <box size="1 0.6 0.3"/>
      </geometry>
    </visual>
  </link>
  



  <link name="carA__link_1">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </visual>
  </link>

  <link name="carA__link_1_clone">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </visual>
  </link>

  <link name="carA__link_1_clone_0">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </visual>
  </link>

  <link name="carA__link_1_clone_1">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.07"/>
      </geometry>
    </visual>
  </link>

  <link name="carA__left_wheel">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.0145833" ixy="0" ixz="0" iyy="0.0145833" iyz="0" izz="0.0125"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
    </visual>
  </link>

  <link name="carA__right_wheel">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.0145833" ixy="0" ixz="0" iyy="0.0145833" iyz="0" izz="0.0125"/>
    </inertial>
    <collision name="carA__collision">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
    </visual>
  </link>

  <!-- Lidar -->
  <link name="carA__front_hokuyo">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://car_description/carA/meshes/hokuyo.dae" />
      </geometry>
    </visual>
  </link>

  <link name="carA__rear_hokuyo">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://car_description/carA/meshes/hokuyo.dae" />
      </geometry>
    </visual>
  </link>

  <!-- camera -->
  <link name="camera">
      <visual>
        <origin xyz="-0.015 0 -0.0257" rpy="1.5707 0 0"/>
        <geometry>
            <mesh filename="package://car_description/carA/meshes/webcam.stl" scale="0.01 0.01 0.01" />
        </geometry>
      </visual>

      <inertial>
        <mass value="1e-5" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4" />
      </inertial>
    </link>

    <joint name="sensor_joint" type="fixed">
        <parent link="carA__base_link"/>
        <child link="camera"/>
        <origin rpy="0 0 0" xyz="0.48 0.0 0.48"/>
        <axis xyz="0 0 1" />
    </joint>

    <!-- IMU -->
    <link name="imu_link">
      <!-- <xacro:hokuyo_params /> -->
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="package://car_description/carA/meshes/hokuyo.dae" scale="0.050 0.050 0.050"/>
        </geometry>
      </visual>

      <inertial>
        <mass value="1e-5" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4" />
      </inertial>
    </link>

    <joint name="imu_joint" type="fixed">
      <origin xyz="0.0 0.0 0.15" rpy="0 0 0"/>
      <parent link="carA__base_link" />
      <child link="imu_link" />
      <axis xyz="0 0 1" />
    </joint>




<!--  -->

</robot>
